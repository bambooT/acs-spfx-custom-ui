{
  "name": "acs-spfx-ui",
  "version": "1.0.10",
  "description": "Acs ui framework for sharepoint framework",
  "main": "dist/lib/index",
  "typings": "dist/lib/index",
  "scripts": {
    "build": "rimraf ./dist && tsc",
    "prepare": "yarn test && yarn lint && yarn build",
    "test": "jest --coverage",
    "lint": "tslint -c tslint.json --project tsconfig.json"
  },
  "husky": {
    "hooks": {
      "pre-push": "yarn test && yarn lint"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yuyuzeze/acs-spfx-custom-ui.git"
  },
  "keywords": [
    "acs",
    "spfx"
  ],
  "author": "Jerry",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yuyuzeze/acs-spfx-custom-ui/issues"
  },
  "homepage": "https://github.com/yuyuzeze/acs-spfx-custom-ui#readme",
  "jest": {
    "testURL": "http://localhost",
    "collectCoverage": true,
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "reporters": [
      "default",
      "jest-junit"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "testMatch": [
      "**/*spec.(ts|tsx)"
    ]
  },
  "peerDependencies": {
    "react": "^16.6.0",
    "react-dom": "^16.6.0"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/enzyme": "^3.1.14",
    "@types/enzyme-adapter-react-16": "^1.0.3",
    "@types/jest": "^23.3.3",
    "@types/react": "^16.4.16",
    "@types/react-dom": "^16.0.8",
    "codecov": "^3.0.4",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.6.0",
    "enzyme-to-json": "^3.3.4",
    "husky": "^1.1.1",
    "jest": "^23.6.0",
    "jest-junit": "^5.2.0",
    "ts-jest": "^23.10.4",
    "tslint": "^5.11.0",
    "tslint-react": "^3.6.0",
    "typescript": "^3.1.1"
  },
  "files": [
    "dist",
    "LICENSE"
  ]
}